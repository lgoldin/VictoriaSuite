  </Fragment>

  <Fragment>        
    <DirectoryRef Id="ApplicationProgramsFolder">
      <Component Id="ApplicationShortcut" Guid="16fcb609-7357-4d39-b8e0-b5b924f61fc5">
        <Shortcut Id="ApplicationStartMenuShortcutVictoria" Name="Victoria" Description="Victoria is a software to simulate everything" Target="[INSTALLFOLDER]$(var.Version)/Victoria.exe" WorkingDirectory="APPLICATIONROOTDIRECTORY" />
        <Shortcut Id="ApplicationStartMenuShortcutKairos" Name="Kairos" Description="Kairos is a ..." Target="[INSTALLFOLDER]Kairos/Kairos.exe" WorkingDirectory="APPLICATIONROOTDIRECTORY" />
        <RemoveFolder Id="RemoveApplicationProgramsFolder" On="uninstall" />
        <RegistryValue Root="HKCU" Key="Software\UTN\$(var.Title)" Name="installed" Type="integer" Value="1" KeyPath="yes" />
      </Component>
            
    </DirectoryRef>
    <DirectoryRef Id="DesktopFolder">
      <Component Id="ApplicationShortcutDesktop" Guid="16fcb609-7357-4d39-b8e0-b5b924f61fc6">
        <Shortcut Id="ApplicationDesktopShortcutVictoria" Name="Victoria" Description="Victoria is a software to simulate everything" Target="[INSTALLFOLDER]$(var.Version)/Victoria.exe" WorkingDirectory="APPLICATIONROOTDIRECTORY" />
        <Shortcut Id="ApplicationDesktopShortcutKairos" Name="Kairos" Description="Kairos is ..." Target="[INSTALLFOLDER]Kairos/Kairos.exe" WorkingDirectory="APPLICATIONROOTDIRECTORY" />
        <RemoveFolder Id="RemoveDesktopFolder" On="uninstall" />
        <RegistryValue Root="HKCU" Key="Software\UTN\$(var.Title)" Name="installed" Type="integer" Value="1" KeyPath="yes" />
      </Component>
    </DirectoryRef>
  </Fragment>

