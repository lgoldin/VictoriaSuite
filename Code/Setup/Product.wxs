  <?define Version="Victoria V"?>
  <?define Title="Victoria Suite"?>
  <Product Id="*" Name="$(var.Title)" Language="1033" Version="5.0.0.1" Manufacturer="UTN" UpgradeCode="16fcb609-7357-4d39-b8e0-b5b924f61fc4">
    <Package InstallerVersion="200" Compressed="yes" InstallScope="perMachine" />

    <Icon Id="icon.ico" SourceFile="$(var.ProjectDir)victoria_suite.ico" />
    <Property Id="ARPPRODUCTICON" Value="icon.ico" />

    <WixVariable Id="WixUIBannerBmp" Value="Images\top-banner.png" />
    <WixVariable Id="WixUIDialogBmp" Value="Images\dialog_utnba_logo.png" />
    
    <WixVariable Id="WixUILicenseRtf" Value="$(var.ProjectDir)License.rtf" />

    <Property Id="WIXUI_INSTALLDIR" Value="INSTALLFOLDER" />
    <UIRef Id="WixUI_InstallDir" />

    <MajorUpgrade Schedule="afterInstallInitialize" AllowSameVersionUpgrades="yes" AllowDowngrades="no" DowngradeErrorMessage="A newer version of [ProductName] is already installed." />
    <MediaTemplate EmbedCab="yes" />

    <Feature Id="ProductFeature" Title="$(var.Title)" Level="1">
      <ComponentGroupRef Id="ProductComponents" />
      <ComponentGroupRef Id="dlls_files" />
      <ComponentRef Id="Logs" />
      <ComponentGroupRef Id="Responsive_HTML5_files" />
      <ComponentGroupRef Id="Responsive_HTML5_Manual_de_usuario_archivos_files" />
      <ComponentGroupRef Id="Responsive_HTML5_template_Azure_Blue_files" />
      <ComponentGroupRef Id="Responsive_HTML5_template_scripts_files" />
      <ComponentGroupRef Id="Responsive_HTML5_template_styles_files" />
      <ComponentGroupRef Id="Responsive_HTML5_whxdata_files" />
      <ComponentGroupRef Id="templates_files" />
      <ComponentRef Id="ApplicationShortcut" />
      <ComponentRef Id="ApplicationShortcutDesktop" />
      <ComponentGroupRef Id="Manual_de_usuario_files" />
      <ComponentGroupRef Id="Resources_files" />      
      <ComponentGroupRef Id="Kairos" />           
    </Feature>        
  </Product>

